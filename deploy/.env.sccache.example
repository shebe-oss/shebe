# sccache S3 Backend Configuration
# Copy to .env.sccache and fill in credentials (DO NOT COMMIT)

SCCACHE_BUCKET=<bucket-name>
SCCACHE_REGION=<bucket-region>
SCCACHE_ENDPOINT=<bucket-endpoint>
SCCACHE_S3_USE_SSL=true

# DigitalOcean Spaces credentials (S3-compatible)
AWS_ACCESS_KEY_ID=<your-access-key>
AWS_SECRET_ACCESS_KEY=<your-secret-key>
